  "scripts": {
    "test": "browserify tests/**/*.spec.js -t [ envify --ECOSYSTEM_BASE_URI placeholder ] -d | tap-closer | smokestack | tap-spec",
    "test-ugly": "browserify tests/**/*.spec.js -t [ envify --ECOSYSTEM_BASE_URI placeholder ] -d | tap-closer | smokestack"
    "seed": "./seed.sh",
    "develop": "node ./livereload.js & npm run start-ecosystem & npm run watch-components & npm run watch-runtime & npm run serve-build",
    "start-ecosystem": "cd node_modules/best-ecosystem && npm run serve",
    "sync-components": "ECOSYSTEM_BASE_URI=http://localhost:8357/ best-assistant sync --baseDir=$PWD/node_modules/best-components/",
    "watch-components": "ECOSYSTEM_BASE_URI=http://localhost:8357/ best-assistant watch --baseDir=$PWD/node_modules/best-components/",
    "watch-runtime": "watchify node_modules/best-runtime/lib/index.js -s BEST -t [ envify --ECOSYSTEM_BASE_URI http://localhost:8357/ ] -o build/best.bundle.js -d -v",
    "serve-build": "serve build -p 1337"
  },  
