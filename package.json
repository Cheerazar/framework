{
  "name": "famous-framework",
  "version": "0.11.3",
  "description": "A modern take on building reusable components and maintainable applications",
  "keywords": [
    "famous",
    "framework"
  ],
  "license": "MIT",
  "repository": "Famous/framework",
  "bugs": "https://github.com/Famous/framework/issues",
  "private": true,
  "main": "lib/index.js",
  "authors": [
    "Zack Brown <zack@famo.us>",
    "Matthew Trost <matthew@famo.us>",
    "Arkady Pevzner <arkady@famo.us>",
    "Imtiaz Majeed <imtiaz@famo.us>",
    "Jordan Papaleo <jordan@famo.us>",
    "Larry Gordon <lg@famo.us>"
  ],
  "bin": {
    "famous-framework": "bin/famous-framework.js"
  },
  "scripts": {
    "dev": "npm run watch-runtime & npm run local-only-bootstrap",
    "local-only-bootstrap": "node bin/famous-framework.js local-only-bootstrap --sourceDirectory=lib/core-components --blocksDirectory=dev/workspace --port=1618",
    "snapshot-component": "node bin/famous-framework.js snapshot-component --sourceDirectory=lib/core-components --blocksDirectory=dev/workspace --destinationDirectory=dev/workspace",
    "watch-runtime": "node bin/famous-framework.js watch-runtime --serverHost=http://localhost:1618 --inputFile=lib/index.js --outputFile=dev/workspace/famous-framework.bundle.js",
    "test": "node bin/famous-framework.js test-runtime && node bin/famous-framework.js test-state-manager && node bin/famous-framework.js test-utilities",
    "test-builder": "browserify dev/tests/builder-tests/*.spec.js | tap-closer | smokestack",
    "test-esprima-heper": "browserify dev/tests/esprima-helper-tests/*.spec.js | tap-closer | smokestack",
    "test-runtime": "browserify dev/tests/runtime-tests/**/*.spec.js | tap-closer | smokestack",
    "test-state-manager": "browserify dev/tests/state-manager-tests/*.spec.js | tap-closer | smokestack",
    "test-utilities": "browserify dev/tests/utilities-tests/*.spec.js | tap-closer | smokestack",
    "lint": "eslint --ignore-path .eslintignore ."
  },
  "dependencies": {
    "async": "1.2.1",
    "babel": "5.5.8",
    "browserify": "10.2.4",
    "chalk": "1.0.0",
    "chokidar": "1.0.3",
    "clone": "1.0.2",
    "commander": "2.8.1",
    "envify": "3.4.0",
    "escodegen": "1.6.1",
    "esprima": "2.2.0",
    "estraverse": "4.1.0",
    "express": "4.12.4",
    "famous": "Famous/engine#v0.6.1",
    "form-data": "matthewtoast/node-form-data#v0.2.1",
    "jsdom": "3.1.2",
    "less": "2.5.1",
    "livereload": "0.3.7",
    "lodash": "3.9.3",
    "mkdirp": "0.5.1",
    "ncp": "2.0.0",
    "node-watch": "0.3.4",
    "nodemon": "1.3.7",
    "osenv": "^0.1.2",
    "recursive-readdir": "1.2.1",
    "request": "2.57.0",
    "rimraf": "^2.4.0",
    "serve": "1.4.0",
    "stylus": "0.51.1",
    "temp": "0.8.3",
    "traverse": "0.6.6",
    "watchify": "3.2.2",
    "xhr": "2.0.2"
  },
  "devDependencies": {
    "babel-eslint": "3.1.15",
    "eslint": "0.23.0",
    "jsdoc": "3.3.2",
    "smokestack": "3.3.0",
    "tap-closer": "1.0.0",
    "tape": "4.0.0"
  }
}
