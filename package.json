{
  "name": "best",
  "version": "0.1.0",
  "description": "Behavior Event State Template",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.famo.us/framework/best.git"
  },
  "bugs": {
    "url": "https://github.famo.us/framework/best/issues"
  },
  "homepage": "https://github.famo.us/framework/best",
  "scripts": {
    "develop": "npm run watch & npm run serve",
    "docs": "jsdoc lib/framework/index.js --destination docs/generated",
    "test": "testling '' | tap-spec",
    "lint": "eslint lib && jscs lib",
    "serve": "serve build -p 1337",
    "copy-components": "cp -r components/ build/components",
    "watch-lib": "watchify lib/index.js -t brfs -s BEST -o build/index.bundle.js --debug --verbose",
    "watch-components": "watch 'npm run copy-components' components",
    "watch-public": "watch 'npm run copy-public' public",
    "watch-all": "npm run watch-public & npm run watch-components & npm run watch-lib",
    "copy-public": "cp -r public/ build",
    "watch": "rm -rf build && mkdir build && npm run watch-all"
  },
  "authors": [
    "Zack Brown <zack@famo.us>",
    "Matthew Trost <matthew@famo.us>",
    "Arkady Pevzner <arkady@famo.us>",
    "Imtiaz Majeed <imtiaz@famo.us>",
    "Jordan Papaleo <jordan@famo.us>"
  ],
  "license": "Proprietary",
  "testling": {
    "files": [
      "tests/**.spec.js"
    ],
    "scripts": [
        "tests/helpers/bind-polyfill.js",
        "tests/helpers/html-domparser.js"
    ]
  },
  "devDependencies": {
    "brfs": "^1.1.1",
    "browserify": "^8.1.3",
    "eslint": "^0.14.1",
    "jasmine-core": "^2.2.0",
    "jscs": "^1.11.3",
    "jsdoc": "^3.3.0-beta1",
    "jshint": "^2.6.0",
    "nodemon": "^1.3.7",
    "serve": "^1.4.0",
    "tap-spec": "^2.2.2",
    "tape": "^3.5.0",
    "testling": "^1.7.1",
    "uglify-js": "^2.4.16",
    "watch": "^0.14.0",
    "watchify": "^2.3.0"
  },
  "dependencies": {
    "best-function-parser": "git+ssh://git@github.famo.us:framework/function-parser#v0.1.0",
    "best-state-manager": "git+ssh://git@github.famo.us:framework/state-manager#v0.1.1",
    "framework-utilities": "git+ssh://git@github.famo.us:framework/utilities.git#v0.2.0",
    "mustache": "^1.1.0",
    "famous-components": "git+ssh://git@github.famo.us/platform/components#v0.1.1",
    "famous-core": "git+ssh://git@github.famo.us/platform/core#v0.2.0",
    "famous-dom-renderers": "git+ssh://git@github.famo.us/platform/dom-renderers#v0.2.0",
    "famous-engine": "git+ssh://git@github.famo.us/platform/engine#v0.1.1",
    "famous-math": "git+ssh://git@github.famo.us/platform/math#v0.1.0",
    "famous-physics": "git+ssh://git@github.famo.us/platform/physics#v0.2.0",
    "famous-renderers": "git+ssh://git@github.famo.us/platform/renderers#v0.2.1",
    "famous-dom-renderables": "git+ssh://git@github.famo.us/platform/dom-renderables#v0.2.0",
    "famous-stylesheets": "git+ssh://git@github.famo.us/platform/stylesheets#v0.2.0",
    "famous-router": "git+ssh://git@github.famo.us/platform/router#v0.1.0",
    "famous-transitions": "git+ssh://git@github.famo.us/platform/transitions#v0.1.1",
    "famous-utilities": "git+ssh://git@github.famo.us/platform/utilities#v0.2.0",
    "famous-webgl-renderables": "git+ssh://git@github.famo.us/platform/webgl-renderables#v0.1.1",
