    "famous-webgl-geometries": "git+ssh://git@github.famo.us/platform/webgl-geometries#v0.1.0",
    "famous-webgl-materials": "git+ssh://git@github.famo.us/platform/webgl-materials#v0.1.0",
    "famous-webgl-shaders": "git+ssh://git@github.famo.us/platform/webgl-shaders#v0.1.1",
    "famous-polyfills": "git+ssh://git@github.famo.us/platform/polyfills#v0.1.0"
  }
}
